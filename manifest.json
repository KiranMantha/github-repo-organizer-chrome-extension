{
  "manifest_version": 3,
  "name": "GitHub Repo Organizer",
  "version": "1.0",
  "description": "Organize GitHub repositories using folders stored in a gist",
  "permissions": ["activeTab", "scripting", "storage", "identity"],
  "host_permissions": ["https://api.github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["dist/content.js"]
    }
  ]
}
